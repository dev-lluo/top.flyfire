package top.flyfire.degetation;

public class Loop {
	public interface Task<T>{
		void run(T t, int index);
		boolean needNext();
	}
	public static <T> void run(final T[] target,Task<T> task){
		Go.to(Validator.Obj.notNull(target)&&Validator.Obj.notNull(task), new Go.Task() {
			
			@Override
			public void run() {
				// TODO Auto-generated method stub
				for(int i = 0;i<target.length;i++){
					
				}
			}
		});
	}
}
