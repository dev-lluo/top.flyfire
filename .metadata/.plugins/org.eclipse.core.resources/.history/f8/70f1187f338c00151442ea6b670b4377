package top.flyfire.degetation;

public final class Go {
	public abstract class Task{
		abstract void run();
	}
	public abstract class SimpleValidator extends Validator<Boolean>{
		abstract Boolean validate();
	}
	public abstract class Validator<T>{
		abstract T validate();
	}
	public static void to(boolean flag,Task task){
		if(flag){
			task.run();
		}
	}
	public static <T> void to(SimpleValidator validator,Task task){
		if(validator.validate()){
			task.run();
		}
	}
}
