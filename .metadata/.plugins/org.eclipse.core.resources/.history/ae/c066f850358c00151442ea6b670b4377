package top.flyfire.degetation;

public class Loop {
	public interface Task<T>{
		void run(T t, int index);
		boolean needNext();
	}
	public static <T> void run(T[] target,Task<T> task){
		Go.to(Validator.Obj.notNull(target), new Go.Task() {
			
			@Override
			void run() {
				// TODO Auto-generated method stub
				
			}
		});
	}
}
