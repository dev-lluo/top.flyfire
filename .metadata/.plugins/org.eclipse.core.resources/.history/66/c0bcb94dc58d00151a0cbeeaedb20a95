package top.flyfire.degetation.console;

import top.flyfire.degetation.go.Go;
import top.flyfire.degetation.go.Go.GoTask;

public class Console implements IConsole {
	private static IConsole console = null;
	static{
		Go.nullTo(console, new GoTask() {
			
			@Override
			public void run() {
				// TODO Auto-generated method stub
				console = new DefaultConsole();
			}
		});
	}
	
	public static IConsole getConsole() {
		return Console.console;
	}
	
	@Override
	public void log(Object obj){
		console.log(obj);
	}
	@Override
	public void warn(Object obj){
		console.warn(obj);
	}
	@Override
	public void error(Object obj){
		console.error(obj);
	}
	@Override
	public void info(Object obj){
		console.info(obj);
	}
	@Override
	public void log(Throwable e){
		console.log(e);
	}
	@Override
	public void warn(Throwable e){
		console.warn(e);
	}
	@Override
	public void error(Throwable e){
		console.error(e);
	}
	@Override
	public void info(Throwable e){
		console.info(e);
	}
	
}
