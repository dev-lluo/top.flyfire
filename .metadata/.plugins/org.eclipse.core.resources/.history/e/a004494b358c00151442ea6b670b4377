package top.flyfire.degetation;

public final class Go {
	public interface Task{
		public void run();
	}
	public class TaskList<T>{
		private KVal<T, Task>[] list;
		public TaskList(KVal<T, Task>...list) {
			// TODO Auto-generated constructor stub
			this.list = list;
		}
		public void choose(T t){
			
		}
	}
	public abstract class SimpleValidator extends Validator<Boolean>{
		abstract Boolean validate();
	}
	public abstract class Validator<T>{
		abstract T validate();
	}
	public static void to(boolean flag,Task task){
		if(flag){
			task.run();
		}
	}
	public static void to(SimpleValidator validator,Task task){
		if(validator.validate()){
			task.run();
		}
	}
	public static <T> void to(Validator<T> validator , TaskList<T> list){
		
	}
}
